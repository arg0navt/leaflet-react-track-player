{
  "name": "leaflet-react-track-player",
  "description": "This is plugin to react-leaflet. It create player for control of track.",
  "version": "2.0.0",
  "private": false,
  "main": "lib/index.js",
  "homepage": "https://github.com/argonavt11/leaflet-react-track-player",
  "license": "MIT",
  "author": "Alexander Zolotarev",
  "bugs": {
    "url": "https://github.com/argonavt11/leaflet-react-track-player/issues",
    "email": "argonavt.al@yandex.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argonavt11/leaflet-react-track-player.git"
  },
  "dependencies": {
    "@skyeer/react-leaflet-custom-control": "^0.0.6",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "leaflet": "^1.6.0",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react-leaflet": "^2.7.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lib": "babel src/laeflet-react-track-player --out-dir lib --copy-files"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "components/ColoredCalendar/([^\\.]*)$": "<rootDir>/src/node_modules/components/ColoredCalendar/$1.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "coveralls": "^3.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "4.18.2",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.13.1",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  }
}
